{
    "name": "tanghengzhi/automating-mysql-partitioning",
    "description": "This is my package automating-mysql-partitioning",
    "keywords": [
        "Victor Tang",
        "laravel",
        "automating-mysql-partitioning"
    ],
    "homepage": "https://github.com/tanghengzhi/automating-mysql-partitioning",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Tang",
            "email": "tanghengzhi@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Victor Tang\\AutomatingMysqlPartitioning\\": "src/",
            "Victor Tang\\AutomatingMysqlPartitioning\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victor Tang\\AutomatingMysqlPartitioning\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Victor Tang\\AutomatingMysqlPartitioning\\AutomatingMysqlPartitioningServiceProvider"
            ],
            "aliases": {
                "AutomatingMysqlPartitioning": "Victor Tang\\AutomatingMysqlPartitioning\\Facades\\AutomatingMysqlPartitioning"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
